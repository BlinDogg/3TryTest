<UserControl x:Class="_3TryTest.UserControlDrift"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:_3TryTest"
             mc:Ignorable="d" d:DesignWidth="200" Height="40">
    <Grid Background="#FDFD96" Width="200" Height="40">
        <Grid.Clip>
            <RectangleGeometry Rect="0,0 200,40" RadiusX="30" RadiusY="30" />
        </Grid.Clip>
        <Grid>
            <Ellipse Width="30" Fill="#FF4500" Margin="85,0,85,0" x:Name="mooveout"/>
            <Line X1="75" Y1="0" X2="75" Y2="40" StrokeThickness="2" Stroke="Black"/>
            <Line X1="125" Y1="0" X2="125" Y2="40" StrokeThickness="2" Stroke="Black"/>
        </Grid>
    </Grid>
</UserControl>
